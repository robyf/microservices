extend type Query {
    # Lending account for current user
    lendingAccount: LendingAccount
}

enum LendingAccountStatus {
    NEW
    ACTIVE
    FROZEN
    CLOSED
}

# A lending account.
type LendingAccount {
    # The account unique identifier.
    id: ID!
    # Status.
    status: LendingAccountStatus!
    # Current account balance.
    balance: Float!
}
